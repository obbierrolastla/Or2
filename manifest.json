{
	"name":"Or2 Extention",
	"description":"Ekstensi chrome pengawas anak",
	"version":"1.0",
	"manifest_version":2,
	"content_scripts":[
		{
			"matches":[
				"https://*/*","http://*/*"
			],
			"js":[
				"ortu.js"
			]
		}
	],
	"browser_action":{
		"default_icon":"icon.png",
                "default_popup":"ortu.html"
	},
	"permissions":[
		"tabs",
		"webRequest",
		"*://*/"
	]
}
